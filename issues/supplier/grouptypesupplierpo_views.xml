<odoo>

	<record id="view_tree_witness" model="ir.ui.view">
		<field name="name">purchase.order.tree1</field>
		<field name="model">purchase.order</field>
		<field name="inherit_id"
			ref="purchase.purchase_order_kpis_tree" />
		<field name="arch" type="xml">
			<field name="company_id" position="after">
				<field name="date_planned" />
			</field>
		</field>
	</record>
	<record id="ms_purchase_order_view_search" model="ir.ui.view">
		<field name="name">purchase.order.select1</field>
		<field name="model">purchase.order</field>
		<field name="inherit_id"
			ref="purchase.view_purchase_order_filter" />
		<field name="arch" type="xml">
			<xpath expr="/search/filter[10]" position="after">
				<filter string="Type Customer/Vendor" name="type_partner_po"
					domain="[]" context="{'group_by': 'type_partner_po'}" />
			</xpath>

		</field>
	</record>

</odoo>